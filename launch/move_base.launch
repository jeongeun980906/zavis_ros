<launch>
    <!-- Arguments -->
    <arg name="cmd_vel_topic" default="/RosAria/cmd_vel" />
    <arg name="odom_topic" default="/RosAria/pose" />
  
    <!-- move_base -->
    <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
        <rosparam file="$(find asw_ros)/config/costmap_common_params.yaml" command="load" ns="global_costmap" /> 
        <rosparam file="$(find asw_ros)/config/global_costmap_params.yaml" command="load" /> 
        <rosparam file="$(find asw_ros)/config/costmap_common_params.yaml" command="load" ns="local_costmap" /> 
        <rosparam file="$(find asw_ros)/config/local_costmap_params.yaml" command="load" /> 
        <rosparam file="$(find asw_ros)/config/dwa_local_planner_params.yaml" command="load" /> 
        <remap from="/cmd_vel" to="/RosAria/cmd_vel"/>
        <remap from="/odom" to="/RosAria/pose"/>
    </node>
  </launch>
